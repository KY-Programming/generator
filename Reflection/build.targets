<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <Target Name="KY-Generator-Reflection_RemoveFromPublish" AfterTargets="ComputeFilesToPublish;ResolvePublishAssets">
        <ItemGroup Condition="'$(GeneratePublish)'=='None'">
            <ResolvedFileToPublish Remove="@(ResolvedFileToPublish)" Condition="'%(ResolvedFileToPublish.Filename)%(ResolvedFileToPublish.Extension)'=='KY.Generator.Reflection.dll'" />
        </ItemGroup>
        <ItemGroup Condition="'$(GeneratePublish)'!='All'">
            <ResolvedFileToPublish Remove="@(ResolvedFileToPublish)" Condition="'%(ResolvedFileToPublish.Filename)%(ResolvedFileToPublish.Extension)'=='KY.Generator.Reflection.Fluent.dll'" />
        </ItemGroup>
    </Target>
</Project>