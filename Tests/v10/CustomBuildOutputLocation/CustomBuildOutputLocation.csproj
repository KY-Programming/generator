    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="KY.Generator" Version="10.0.0-preview.3" />
      <PackageReference Include="KY.Generator.Annotations" Version="10.0.0-preview.3" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Output\" />
    </ItemGroup>

</Project>
