<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <Target Name="KY-Generator-EntityFramework_RemoveFromPublish" AfterTargets="ComputeFilesToPublish;ResolvePublishAssets">
        <ItemGroup Condition="'$(GeneratePublish)'=='None'">
            <ResolvedFileToPublish Remove="@(ResolvedFileToPublish)" Condition="'%(ResolvedFileToPublish.Filename)%(ResolvedFileToPublish.Extension)'=='KY.Generator.EntityFramework.dll'" />
        </ItemGroup>
        <ItemGroup Condition="'$(GeneratePublish)'!='All'">
            <!-- <ResolvedFileToPublish Remove="@(ResolvedFileToPublish)" Condition="'%(ResolvedFileToPublish.Filename)%(ResolvedFileToPublish.Extension)'=='KY.Generator.EntityFramework.Fluent.dll'" />-->
            <ResolvedFileToPublish Remove="@(ResolvedFileToPublish)" Condition="'%(ResolvedFileToPublish.Filename)%(ResolvedFileToPublish.Extension)'=='KY.Generator.EntityFramework.dll'" />
        </ItemGroup>
    </Target>
</Project>