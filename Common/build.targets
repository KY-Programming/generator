<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <Target Name="KY.Generator.Common_RemoveFromPublish" AfterTargets="ComputeFilesToPublish;ResolvePublishAssets">
        <ItemGroup Condition="'$(GeneratePublish)'=='None'">
            <ResolvedFileToPublish Remove="@(ResolvedFileToPublish)" Condition="'%(ResolvedFileToPublish.Filename)%(ResolvedFileToPublish.Extension)'=='Package.dll'" />
        </ItemGroup>
        <ItemGroup Condition="'$(GeneratePublish)'!='All'">
            <ResolvedFileToPublish Remove="@(ResolvedFileToPublish)" Condition="'%(ResolvedFileToPublish.Filename)%(ResolvedFileToPublish.Extension)'=='Package.Syntax.dll'" />
        </ItemGroup>
    </Target>
</Project>